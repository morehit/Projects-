<h1 class="author__heading"> Search Results</h1>

<article >

    
    <ul class="article-ul">

        <% data.forEach(post => { %>

        <li>
            <a href="/post/<%= post._id %>">
                <span> <%= post.title %></span>
                <span class="article-list__data"><%= post.createdAt.toDateString()%></span>
            </a>

        </li>
        <% }) %>

    </ul>
</article>